{
	"compilerOptions": {
		"target": "esnext",
		"moduleResolution": "nodenext",
		"strict": true,
		"skipDefaultLibCheck": true,
		"skipLibCheck": true,
		"rootDir": "./src",
		"noImplicitAny": false,
		"allowImportingTsExtensions": true,
		"noEmit": true,
		"allowSyntheticDefaultImports": false,
		"verbatimModuleSyntax": true,
		"module": "nodenext"
	}
}
